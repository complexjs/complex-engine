{
  "name": "complex-engine",
  "description": "JS Component Entity Framework for Gamedevelopment",
  "license": "MIT",
  "version": "6.0.1",
  "keywords": [
    "engine",
    "game",
    "gamedev",
    "development",
    "html5",
    "gameengine",
    "indie",
    "component",
    "entity",
    "system"
  ],
  "author": {
    "name": "Fabio Gianini"
  },
  "repository": {
    "url": "https://github.com/faebeee/complex-engine"
  },
  "main": "./build/bundle.js",
  "types": "./build/Complex.d.ts",
  "typings": "./build/Complex.d.ts",
  "files": [
    "build/**/*"
  ],
  "devDependencies": {
    "mocha": "^3.0.2",
    "sinon": "^5.0.7",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1",
    "typescript-bundle": "^0.8.9",
    "unit.js": "^2.0.0"
  },
  "scripts": {
    "build": "tsc && npm run bundle && npm run doc",
    "bundle": "npx tsc-bundle src/Complex.ts build/bundle.js --exportAs cx --target es5",
    "test": "mocha test/**/*.test.js --recursive",
    "doc": "npx typedoc --out docs ./src --mode file --readme ./README.md --name Complex"
  }
}
